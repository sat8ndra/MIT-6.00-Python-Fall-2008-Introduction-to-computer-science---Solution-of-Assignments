
mcnugget = (50,51,52,53,54,55)
a = 0
b = 0
c = 0
for i in range(0,5) :
    for a in range(0,10):
        for b in range(0,7) :
            for c in range(0,3) :
                if (6*a + 9*b + 20*c) == mcnugget[i] :
                    print('for '+ str(mcnugget[i]), a,b,c)
                
                    
